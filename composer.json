{
    "name": "assoconnect/enqueue-azure",
    "type": "library",
    "description": "Message Queue Azure Storage Transport",
    "keywords": ["messaging", "queue", "azure", "storage"],
    "homepage": "https://github.com/assoconnect/",
    "license": "MIT",
    "require": {
        "php": "^7.1.3",
        "queue-interop/queue-interop": "0.7.x-dev",
        "microsoft/azure-storage-queue": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.4.0",
        "enqueue/test": "0.9.x-dev",
        "enqueue/null": "0.9.x-dev",
        "queue-interop/queue-spec": "0.6.x-dev"
    },
    "support": {
        "email": "github@assoconnect.com",
        "issues": "https://github.com/assoconnect/enqueue-azure/issues",
        "source": "https://github.com/assoconnect/enqueue-azure/",
        "docs": "https://github.com/assoconnect/enqueue-azure/blob/master/docs/index.md"
    },
    "autoload": {
        "psr-4": { "Enqueue\\AzureStorage\\": "" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "0.0.x-dev"
        }
    }
}
